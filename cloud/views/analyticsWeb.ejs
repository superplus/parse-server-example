{
  "x_axis": {
    "labels": [
      "D1 (<%= (d1*100).toFixed(2)%>%)",
      "D7 (<%= (d7*100).toFixed(2)%>%)",
      "D14 (<%= (d14*100).toFixed(2)%>%)",
      "D30 (<%= (d30*100).toFixed(2)%>%)"
    ]
  },
  "y_axis": {
    "format": "percent"
  },
  "series": [
    {
      "data": [
        <%= d1 %>,
        <%= d7 %>,
        <%= d14 %>,
        <%= d30 %>
      ]
    }
  ]
}